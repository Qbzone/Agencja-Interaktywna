@model Agencja_Interaktywna.Models.EmployeeContract
@{
    ViewData["Title"] = "Profile";
}

<h1>Profil</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Profile">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="control-label">Imię: @Html.DisplayFor(model => model.IdPracownikNavigation.IdPracownikNavigation.Imie)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Nazwisko: @Html.DisplayFor(model => model.IdPracownikNavigation.IdPracownikNavigation.Nazwisko)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Numer telefonu prywatny:  @Html.DisplayFor(model => model.IdPracownikNavigation.IdPracownikNavigation.NumerTelefonuPrywatny)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Numer telefonu służbowy:  @Html.DisplayFor(model => model.IdPracownikNavigation.IdPracownikNavigation.NumerTelefonuSluzbowego)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Adres e-mail: @Html.DisplayFor(model => model.IdPracownikNavigation.IdPracownikNavigation.AdresEmail)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Stanowisko: @Html.DisplayFor(model => model.IdPracownikNavigation.IdPracownikNavigation.Rola)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Pesel: @Html.DisplayFor(model => model.IdPracownikNavigation.Pesel)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Pensja: @Html.DisplayFor(model => model.IdPracownikNavigation.Pensja)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Umowa: @Html.DisplayFor(model => model.IdUmowaNavigation.RodzajUmowy)</label>
            </div>
        </form>
    </div>
</div>
