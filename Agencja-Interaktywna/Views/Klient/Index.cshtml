@model Agencja_Interaktywna.Models.Klient
@{
    ViewData["Title"] = "Index";
}

<h1>Index Klienta</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Contact">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="control-label">
                    Imię: @Html.DisplayNameFor(model => model.IdKlientNavigation.AdresEmail)
                </label>
            </div>
            <div class="form-group">
                <label class="control-label">Nazwisko: </label>
            </div>
            <div class="form-group">
                <label class="control-label">Numer telefonu prywatny: </label>
            </div>
            <div class="form-group">
                <label class="control-label">Numer telefonu służbowy: </label>
            </div>
            <div class="form-group">
                <label class="control-label">Adres e-mail: </label>
            </div>
        </form>
    </div>
</div>

