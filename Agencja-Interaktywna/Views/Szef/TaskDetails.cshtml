@model Agencja_Interaktywna.Models.UslugaProjekt
@{
    ViewData["Title"] = "TaskDetails";
}

<h1>Szczegóły Zadania</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="TaskDetails">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label class="control-label">Nazwa zadania: @Html.DisplayFor(model => model.IdUslugaNavigation.Nazwa)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Opis zadania: @Html.DisplayFor(model => model.Opis)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Data rozpoczęcia zadania:  @Html.DisplayFor(model => model.DataPrzypisaniaZadania)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Termin oddania zadania:  @Html.DisplayFor(model => model.DataZakonczeniaZadania)</label>
            </div>
            <div class="form-group">
                <label class="control-label">Status zadania: @Html.DisplayFor(model => model.Status)</label>
            </div>
        </form>
    </div>
</div>
<a href="Index" onClick="history.go(-1); return false;"> Wróć</a>