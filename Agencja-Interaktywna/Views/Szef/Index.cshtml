@model List<Agencja_Interaktywna.Models.Projekt>
@{
    ViewData["Title"] = "Index";
}

<h3>Witaj @ViewBag.userEmail </h3>

<table>
    <tbody>
        @foreach (var item in @Model)
        {
            <tr>
                <td>
                    <img src="/@item.Logo" alt="/@item.Logo" height="100" width="100" />
                </td>
                <td>
                    @item.Nazwa
                </td>
                <td>
                    <a asp-action="ProjectDetails" asp-route-id="@item.IdProjekt">Podgląd</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<td>
    <a asp-action="ProjectCreate" class="btn btn-primary">Dodaj nowy projekt</a>
</td>