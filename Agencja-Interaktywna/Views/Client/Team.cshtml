@model List<Interactive_Agency.Models.EmployeeTeam>
@{
    ViewData["Title"] = "Teams";
}

<h1>Lista pracowników</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Imię</th>
            <th scope="col">Nazwisko</th>
            <th scope="col">Pełniona rola</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in @Model)
        {
            <tr>
                <td>
                    @item.EmployeeIdNavigation.EmployeeIdNavigation.Imie
                </td>
                <td>
                    @item.EmployeeIdNavigation.EmployeeIdNavigation.Nazwisko
                </td>
                <td>
                    @item.EmployeeIdNavigation.EmployeeIdNavigation.Rola
                </td>

            </tr>
        }
    </tbody>
</table>
<a href="ProjectDetails" onClick="history.go(-1); return false;" class="btn btn-primary">Wstecz</a>