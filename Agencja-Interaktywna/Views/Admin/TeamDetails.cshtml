@model List<Interactive_Agency.Models.EmployeeTeam>
@{
    ViewData["Title"] = "Team";
}

<h1>Team member</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">First name</th>
            <th scope="col">Second name</th>
            <th scope="col">Role</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in @Model)
        {
            <tr>
                <td>
                    @item.EmployeeIdNavigation.EmployeeIdNavigation.FirstName
                </td>
                <td>
                    @item.EmployeeIdNavigation.EmployeeIdNavigation.LastName
                </td>
                <td>
                    @item.EmployeeIdNavigation.EmployeeIdNavigation.Role
                </td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Teams" class="btn btn-primary">Return</a>